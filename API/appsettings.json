    "RefreshTokenExpirationDays": 7
  },
  "Cassandra": {
    "ContactPoints": "localhost",
    "Port": 9042,
    "Keyspace": "logging",
    "Username": "",
    "Password": "",
    "ReplicationStrategy": "SimpleStrategy",
    "ReplicationFactor": 1
  },
  "AllowedHosts": "*"
}